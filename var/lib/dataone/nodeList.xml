    <node replicate="true" synchronize="true" type="cn" environment="dev">
        <identifier>cn-dev</identifier>
        <name>cn_dev</name>
        <description>Development Coordinating node</description>
        <baseURL>http://cn-dev.dataone.org/cn/</baseURL>
        <services>
            <service version="0.5" available="true">
                <name>cn_crud</name>
                <method name="get" rest="object/${GUID}" implemented="true" />
                <method name="getSystemMetadata" rest="meta/${GUID}" implemented="true" />
                <method name="resolve" rest="resolve/${GUID}" implemented="true" />
            </service>
        </services>
        <synchronization>
            <schedule sec="0" min="10,40" hour="*" mon="*" mday="*" wday="*" year="*"/>	
            <lastHarvested>2010-08-23T11:11:11.123456</lastHarvested>
            <lastCompleteHarvest>2010-08-21T12:12:12.123456</lastCompleteHarvest>
        </synchronization>
        <health state="up">
            <ping success="true" lastSuccess="2010-08-23T11:11:11.123456"/>
            <status dateChecked="2010-08-23T11:11:11.123456" success="false"/>
        </health>
    </node>
