<VirtualHost *:80>
        DocumentRoot /var/www
        ServerName cn-dev.dataone.org
	DirectoryIndex listObjects.xml

	<Directory /var/www/>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride None
		Order allow,deny
		allow from all
	</Directory>

	Include /etc/apache2/jk_mount

</VirtualHost>

