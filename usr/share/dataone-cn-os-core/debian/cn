<VirtualHost *:80>
        DocumentRoot /var/www
        ServerName SERVER_NAME
	DirectoryIndex listObjects.xml

	<Directory /var/www/>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride None
		Order allow,deny
		allow from all
	</Directory>

	Include /etc/apache2/jk_mount
	
	AllowEncodedSlashes On
	AcceptPathInfo On

</VirtualHost>

